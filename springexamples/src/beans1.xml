<?xml version="1.0" encoding="UTF-8"?>

<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
               
       <bean id="helloWorld" class="springexamples.HelloWorld"
       init-method="init">
        	<property name="s" value="Hello World!"/>                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
        </bean>
        
        <bean id="emp" class="springexamples.Employee" scope="prototype"
        init-method="startMethod">
        	<property name="name" value="Arun Kumar"/>
        	<property name="dept" value="Development"/>
        	
        </bean>
        
        <bean id="stu" class="springexamples.Student" init-method="init"/>
        
        <bean class="springexamples.InitBeans"/>
        
        
  </beans>